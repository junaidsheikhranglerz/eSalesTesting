@model eSalesPro.Models.Warehouse

<div id="EditWarehouse" class="win text-center">
    @*<h3 class="black-color"><b>Find Supplier</b></h3>*@

    <form action="@Url.Action("Edit_Warehouse","Product", new { id = @Model.Id })" class="form-horizontal form-label-left" method="post">

        <div class="col-md-12 margin-category">

            <div class="form-group">
                <label class="supplier-create-texts font-16" for="middle-name">Name</label>
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <input class="width-200" value="@Model.Name" type="text" name="name" id="category">
                </div>
            </div>

            <div class="form-group">
                <label class="supplier-create-texts font-16" for="middle-name">Shelf No.</label>
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <input class="width-200" type="text" value="@Model.shelf_number" name="shelf" id="">
                </div>
            </div>

            <div class="form-group">
                <label class="supplier-create-texts font-16" for="middle-name">Address</label>
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <input class="width-200" type="text" value="@Model.Address" name="address" id="category">
                </div>
            </div>
        </div>



        <div class="col-md-12">
            <div class="col-md-3"></div>

            <div class="col-md-3">
                <input type="submit" value="Save" class="btn btn-success btn-mini center-block width-123" />
            </div>

            <div class="col-md-3">
                <a href="" onclick="cancel_btn()" class="btn btn-primary blue-btn-style btn-mini width-123 center-block a-cancel height-40 mt-0 ml-22">Cancel</a>
            </div>
            <div class="col-md-3"></div>



        </div>
    </form>


</div>