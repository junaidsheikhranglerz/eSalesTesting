@model  List<eSalesPro.Models.Product>
<body id="body-index">
    <div id="sticky-footer-wrap">

        @*-----------------Top Navigation Start---------------*@

        @{
            Html.RenderPartial("Top_Navigation_View");
        }

        @*-----------------Top Navigation End---------------*@

        <div class="container-fluid main-container">

            @*-----------------Side Navigation Start---------------*@
            <div class="row">
                @{
                    Html.RenderPartial("Side_Navigation_View");
                }
                @*-----------------Side Navigation End---------------*@



                @*-----------------Main Container Start---------------*@
                <div class="user-page-height">
                    <div class="bggg-img">
                        <div>

                            <div class="col-md-12">

                                <div class="col-md-3">
                                    <h3 class="bold margin-stock-manager-heading">Stock Manager<small></small></h3>
                                </div>
                                <div class="col-md-3"></div>

                                <div class="col-md-3 pull-right">
                                    <a href="~/Product/Stock_Taking_List" class="btn btn-primary btn-mini width-170 product-stock-btn bold">Update Stocktaking</a>
                                </div>

                                <div class="col-md-3 pull-right">
                                    <a href="~/Home/Epos" class="btn btn-primary btn-mini width-170 product-import-btn bold">Import / Export</a>
                                </div>



                            </div>
                        </div>

                        <div class="clearfix"></div>

                        <div class="col-md-12">

                            <div class="col-md-3 pull-left">
                                <a href="~/Home/Epos" class="btn btn-primary btn-mini supplier-epos-btn bold">Back to EPOS Main Panel</a>
                            </div>

                            <div class="col-md-3 pull-left">
                                <a href="#" data-toggle="modal" data-target="#CreateModal" class="btn btn-primary btn-mini width-200 ml--50 product-import-btn bold">Add New Item</a>

                            </div>


                            <div class="col-md-4 search-filter-div">
                                <label class="supplier-create-texts ml-22" for="middle-name">Search Filter</label>
                                <div class="col-md-3 col-sm-6 col-xs-12">
                                    <input type="text" class="ml--95 width-208" name="postcode" id="postcode">
                                    <a href="#"><img class="search-btn-product" src="~/img/images/ssearch.png" /></a>
                                </div>
                            </div>

                            <div class="clearfix"></div>

                        </div>


                        <div class="clearfix"></div>

                        <div class="margin-checkboxes-product">
                            <div class="row1 pull-right">
                                <div class="pull-left bold  mr-5">Custom Fields</div>
                                <div class="pull-left mr-5"><label class="unbold"><input onclick="normalprice()" id="checkbox-normal" type="checkbox" />Normal</label></div>
                                <div class="pull-left mr-5"><label class="unbold"><input onclick="normalprice()" id="checkbox-trade" type="checkbox" />Trade</label></div>
                                <div class="pull-left mr-5"><label class="unbold"><input onclick="normalprice()" id="checkbox-premium" type="checkbox" />Premium</label></div>
                                <div class="pull-left mr-5"><label class="unbold"><input onclick="normalprice()" id="checkbox-other" type="checkbox" />Other</label></div>
                                <div class="pull-left mr-5"><label class="unbold"><input onclick="normalprice()" id="checkbox-cost" type="checkbox" />Cost</label></div>
                            </div>
                        </div>

                        <div class="clearfix"></div>
                        
                       
                        


                        @*<section id="no-more-tables">*@

                        <section id="no-more-tables" class="margin-table-product">
                            <div class="table-border">

                                <table class="table table-responsive col-md-12">
                                    <thead>
                                        <tr class="table-heading-fonts table-main-headings">
                                            <th>Id</th>
                                            <th>SKU</th>
                                            <th>Description</th>
                                            <th>Normal Price</th>
                                            <th>Trade Price</th>
                                            <th>Premium Price</th>
                                            <th>Other Price</th>
                                            <th>Cost Price</th>
                                            <th>Courier Type</th>
                                            <th id="check-normal-price-th" class="hideDiv">Total Normal Price</th>
                                            <th id="check-trade-price-th" class="hideDiv">Total Trade Price</th>
                                            <th id="check-premium-price-th" class="hideDiv">Total Premium Price</th>
                                            <th id="check-other-price-th" class="hideDiv">Total Other Price</th>
                                            <th id="check-cost-price-th" class="hideDiv">Total Cost Price</th>



                                        </tr>
                                    </thead>



                                    <tbody class="bold tbody-font-style">

                                        @foreach (var v in Model) { 

                                        @*<tr data-toggle="modal" data-target=".bs-example-modal-lg">*@
                                        
                                            <tr class="" >
                                            


                                                <td data-title="User ID">@v.Id</td>
                                                <td data-title="User ID">@v.Custom_Label_</td>
                                                <td data-title="First Name">@v.Item_Title</td>
                                                <td data-title="Last Name">@v.Price_Normal</td>
                                                <td data-title="Email" class="user-emails">@v.Price_Trade</td>
                                                <td data-title="Date Registered">@v.Price_Premium</td>
                                                <td class="">@v.Price_Others</td>
                                                <td class="">@v.Cost_Price</td>
                                                <td class="">@v.Courier_Type</td>
                                                <td id="check-normal-price-td" class="hideDiv">@v.Normal_VAT</td>
                                                <td id="check-trade-price-td" class="hideDiv">@v.Normal_VAT</td>
                                                <td id="check-premium-price-td" class="hideDiv">@v.Normal_VAT</td>
                                                <td id="check-other-price-td" class="hideDiv">@v.Normal_VAT</td>
                                                <td id="check-cost-price-td" class="hideDiv">@v.Normal_VAT</td>
                                        </tr>
                                        }
                                    </tbody>
                                </table>

                                <div class="clearfix"></div>


                            </div>



                        </section>





                    </div>
                </div>
                @*-----------------Main Container Ends---------------*@
            </div>


            <div id="sticky-footer-push"></div>
        </div>
    </div>

    <footer class="black">

        @{
            Html.RenderPartial("Footer_View");
        }
    </footer>



