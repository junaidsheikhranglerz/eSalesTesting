    @*----------------------------POP UP for COMPOSITE ITEMS--------------------------------*@

    <div class="modal fade bs-example-modal-md z-999999999999999999999999" id="composite_modal" tabindex="-1" role="dialog" aria-hidden="false">
        <div class="modal-dialog modal-lg-2">
            <div class="modal-content modal-style-custom">


                <div class="title col-md-12">
                    <span>Composite Items</span>
                    @*<img src="~/img/images/c3.png" class="close mr--13 pull-right" data-dismiss="modal" />*@
                </div>

                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">

                        <div class="win text-center">

                            <p class="mt-30 bold">
                                Add Composite items
                                <a class="ml-5 mr-5" href="#">
                                    <img class="size-35-32 ml-5 mr-5" src="~/img/images/plus.png" />Add Items
                                </a>
                            </p>

                            <form action="" class="form-horizontal form-label-left" method="post">

                                <div class="col-md-12">
                                    <div class="ml-75">
                                        <div class="form-group pull-left">
                                            <label class="control-label pull-left mt--4">SKU:</label>


                                            <input id="sku_composite" class="form-control pull-left width-170" type="text" name="sku_composite" placeholder="" required="" title="admin" style="height: 25px">



                                        </div>

                                        <div class="form-group pull-left">
                                            <label class="control-label pull-left ml-31 mt--4">QTY:</label>


                                            <input id="sku_composite" class="form-control pull-left width-170" type="text" name="sku_composite" placeholder="" required="" title="admin" style="height: 25px">



