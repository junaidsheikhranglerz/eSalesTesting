@model  List<eSalesPro.Models.Product>
<body id="body-index">
    @{
        byte[] buffer = Guid.NewGuid().ToByteArray();
        var FormNumber = BitConverter.ToUInt32(buffer, 0) ^ BitConverter.ToUInt32(buffer, 4) ^ BitConverter.ToUInt32(buffer, 8) ^ BitConverter.ToUInt32(buffer, 12);
        var barcode = FormNumber.ToString("X");
    }




    <div id="sticky-footer-wrap">

        @*-----------------Top Navigation Start---------------*@

        @{
            Html.RenderPartial("Top_Navigation_View");
        }

        @*-----------------Top Navigation End---------------*@

        <div class="container-fluid main-container">

            @*-----------------Side Navigation Start---------------*@
            <div class="row">
                @{
                    Html.RenderPartial("Side_Navigation_View");
                }
                @*-----------------Side Navigation End---------------*@



                @*-----------------Main Container Start---------------*@
                <div class="user-page-height">
                    <div class="bggg-img">
                        <div>

                            <div class="col-md-12">

                                <div class="col-md-3">
                                    <h3 class="bold margin-stock-manager-heading">Stock Manager<small></small></h3>
                                </div>
                                @*<div class="col-md-3">
                                    <a href="#" data-toggle="modal" data-target="#EditModal" class="btn btn-primary blue-btn-style btn-mini width-170 product-stock-btn bold">Edit</a>
                                </div>*@

                                <div class="col-md-3 pull-right">
                                    <a href="~/Product/Stock_Taking_List" class="btn btn-primary blue-btn-style btn-mini width-170 product-stock-btn bold">Update Stocktaking</a>
                                </div>

                                <div class="col-md-3 pull-right">
                                    <a href="~/Home/Epos" class="btn btn-primary btn-mini blue-btn-style width-170 product-import-btn bold">Import / Export</a>
                                </div>



                            </div>
                        </div>

                        <div class="clearfix"></div>

                        <div class="col-md-12">

                            <div class="col-md-3 pull-left">
                                <a href="~/Home/Epos" class="btn btn-primary blue-btn-style btn-mini supplier-epos-btn bold">Back to EPOS Main Panel</a>
                            </div>

                            <div class="col-md-3 pull-left">
                                <a href="#" data-toggle="modal" data-target="#CreateModal" class="btn btn-primary blue-btn-style btn-mini width-200 ml--50 product-import-btn bold">Add New Item</a>

                            </div>


                            <div class="col-md-4 search-filter-div">
                                <label class="supplier-create-texts ml-22" for="middle-name">Search Filter</label>
                                <div class="col-md-3 col-sm-6 col-xs-12">
                                    <input type="text" class="ml--95 width-208" name="postcode" id="postcode">
                                    <a href="#"><img class="search-btn-product" src="~/img/images/ssearch.png" /></a>
                                </div>
                            </div>

                            <div class="clearfix"></div>

                        </div>


                        <div class="clearfix"></div>

                        <div class="margin-checkboxes-product">
                            <div class="row1 pull-right">
                                <div class="pull-left bold  mr-5">Custom Fields</div>
                                <div class="pull-left mr-5"><label class="unbold"><input onclick="normalprice()" id="checkbox-normal" type="checkbox" />Normal</label></div>
                                <div class="pull-left mr-5"><label class="unbold"><input onclick="normalprice()" id="checkbox-trade" type="checkbox" />Trade</label></div>
                                <div class="pull-left mr-5"><label class="unbold"><input onclick="normalprice()" id="checkbox-premium" type="checkbox" />Premium</label></div>
                                <div class="pull-left mr-5"><label class="unbold"><input onclick="normalprice()" id="checkbox-other" type="checkbox" />Other</label></div>
                                <div class="pull-left mr-5"><label class="unbold"><input onclick="normalprice()" id="checkbox-cost" type="checkbox" />Cost</label></div>
                            </div>
                        </div>

                        <div class="clearfix"></div>





                        @*<section id="no-more-tables">*@

                        <section id="no-more-tables" class="margin-table-product">
                            <div class="table-border">

                                <table class="table table-responsive col-md-12">
                                    <thead>
                                        <tr class="table-heading-fonts table-main-headings">
                                            @*<th>Id</th>*@
                                            <th>SKU</th>
                                            <th>Description</th>
                                            <th>Normal Price</th>
                                            <th>Trade Price</th>
                                            <th>Premium Price</th>
                                            <th>Other Price</th>
                                            <th>Cost Price</th>
                                            
                                            <th id="check-normal-price-th" class="hideDiv">Total Normal Price</th>
                                            <th id="check-trade-price-th" class="hideDiv">Total Trade Price</th>
                                            <th id="check-premium-price-th" class="hideDiv">Total Premium Price</th>
                                            <th id="check-other-price-th" class="hideDiv">Total Other Price</th>
                                            <th id="check-cost-price-th" class="hideDiv">Total Cost Price</th>
                                            <th class="text-center">Actions</th>


                                        </tr>
                                    </thead>



                                    <tbody class="bold tbody-font-style">

                                        @foreach (var v in Model)
                                        {

                                            @*<tr data-toggle="modal" data-target=".bs-example-modal-lg">*@

                                            <tr class="">



                                                @*<td data-title="User ID">@v.Id</td>*@
                                                <td data-title="User ID">@v.Custom_Label_</td>
                                                <td data-title="First Name">@v.Item_Title</td>
                                                <td data-title="Last Name">@v.Price_Normal</td>
                                                <td data-title="Email" class="user-emails">@v.Price_Trade</td>
                                                <td data-title="Date Registered">@v.Price_Premium</td>
                                                <td class="">@v.Price_Others</td>
                                                <td class="">@v.Cost_Price</td>
                                                
                                                <td id="check-normal-price-td" class="hideDiv">@v.Normal_VAT</td>
                                                <td id="check-trade-price-td" class="hideDiv">@v.Normal_VAT</td>
                                                <td id="check-premium-price-td" class="hideDiv">@v.Normal_VAT</td>
                                                <td id="check-other-price-td" class="hideDiv">@v.Normal_VAT</td>
                                                <td id="check-cost-price-td" class="hideDiv">@v.Normal_VAT</td>
                                                <td>
                                                    <a href="#" data-toggle="modal" data-target="#EditModal" class="btn btn-sm btn-success bold" onclick="GetProduct(@v.Id)">Edit</a>
                                                <a class="btn btn-sm btn-danger bold" href="">
                                                    Delete
                                                </a>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>

                                <div class="clearfix"></div>


                            </div>



                        </section>

                    </div>
                </div>
                @*-----------------Main Container Ends---------------*@
            </div>


            <div id="sticky-footer-push"></div>
        </div>
    </div>

    <footer class="black">

        @{
            Html.RenderPartial("Footer_View");
        }
    </footer>


    @*----------------------------POP UP for BARCODE--------------------------------*@

    <div class="modal fade bs-example-modal-md z-999999999999999999999999" id="barcode_modal" tabindex="-1" role="dialog" aria-hidden="false">
        <div class="modal-dialog modal-lg-2">
            <div class="modal-content modal-style-custom">

                <div class="title col-md-12">
                    <span>Confirm</span>
                    <img src="~/img/images/c3.png" class="close mr--13 pull-right" />
                </div>

                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">

                        <div class="win text-center">
                            @*<h3 class="black-color"><b>Find Supplier</b></h3>*@


                                <div class="col-md-12">
                                    <div class="empty-height"></div><div class="empty-height"></div><div class="empty-height"></div><div class="empty-height"></div>
                                        <label class="font-16 text-center" for="middle-name">Are you sure you want to use this barcode ?</label>
                                    <br/>
                                    <label class="font-17 text-center black-color" id="barcode_label" for="middle-name">@barcode</label>

                                    <div class="empty-height"></div><div class="empty-height"></div>

                                </div>

                                <div class="col-md-12">
                                    <div class="col-md-3"></div>

                                    <div class="col-md-3">
                                        <input type="button" onclick="Barcode_Ok();" value="Ok" class="btn btn-success btn-mini center-block width-123" />
                                    </div>

                                    <div class="col-md-3">
                                        <a href="#" onclick="cancel_btn()" class="btn btn-primary btn-mini width-123 center-block a-cancel height-40 mt-0 ml-22">Cancel</a>
                                    </div>
                                    <div class="col-md-3"></div>



                                </div>


                        </div>


                    </div>
                </div>

                <div class="modal-footer">


                </div>



            </div>
        </div>
    </div>


    @*----------------------------POP UP END BARCODE--------------------------------*@


    @*----------------------------POP UP for EDIT SUPPLIER--------------------------------*@




    @*----------------------------POP UP END EDIT SUPPLIER--------------------------------*@


    @*----------------------------POP UP for CATEGORY--------------------------------*@

    <div class="modal fade bs-example-modal-md z-999999999999999999999999" id="create_category_modal" tabindex="-1" role="dialog" aria-hidden="false">
        <div class="modal-dialog modal-lg-2">
            <div class="modal-content modal-style-custom">

                <div class="title col-md-12">
                    <span>Categories</span>
                    <img src="~/img/images/c3.png" class="close mr--13 pull-right" />
                </div>

                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">

                        <div class="win text-center">
                            @*<h3 class="black-color"><b>Find Supplier</b></h3>*@

                            <form action="" class="form-horizontal form-label-left" method="post">

                                <div class="col-md-12 margin-category">

                                    <div class="form-group">
                                        <label class="supplier-create-texts font-16" for="middle-name">Category Name<span class="required">*</span></label>
                                        <div class="col-md-3 col-sm-6 col-xs-12">
                                            <input class="width-200" type="text" name="category_title" id="category" required>
                                        </div>
                                    </div>




                                </div>

                                <div class="col-md-12">
                                    <div class="col-md-3"></div>

                                    <div class="col-md-3">
                                        <input type="button" onclick="addCategorys();" value="Save" class="btn btn-success btn-mini center-block width-123" />
                                    </div>

                                    <div class="col-md-3">
                                        <a href="#" onclick="cancel_btn()" class="btn btn-primary btn-mini width-123 center-block a-cancel height-40 mt-0 ml-22">Cancel</a>
                                    </div>
                                    <div class="col-md-3"></div>



                                </div>
                            </form>


                        </div>


                    </div>
                </div>

                <div class="modal-footer">


                </div>



            </div>
        </div>
    </div>


    @*----------------------------POP UP END CATEGORY--------------------------------*@


    @*----------------------------POP UP for STOCK TRANSFER--------------------------------*@

    <div class="modal fade bs-example-modal-md z-999999999999999999999999" id="transfer_stock" tabindex="-1" role="dialog" aria-hidden="false">
        <div class="modal-dialog modal-lg-2">
            <div class="modal-content modal-style-custom">

                <div class="title col-md-12">
                    <span>Stock Transfer</span>
                    @*<img src="~/img/images/c3.png" class="close mr--13 pull-right" data-dismiss="modal" />*@
                </div>

                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">

                        <div class="win text-center">
                            @*<h3 class="black-color"><b>Find Supplier</b></h3>*@

                            <form action="~/Product/List" class="form-horizontal form-label-left" method="post">

                                <div class="col-md-12 margin-stock-modal">

                                    <p class="margin-stock-modal-heading bold font-16">Please select the stock locations. </p>

                                    <div class="form-group">
                                        <label class="stock-transfer-texts" for="middle-name">From:</label>
                                        <div class="col-md-3 col-sm-6 col-xs-12">
                                            <select id="from_warehouse" name="From_Warehouse" class="form-control width-365">
                                                <option value="">Please select any </option>
                                                @foreach (var warehouse in ViewBag.Warehouses)
                                                { 
                                                <option value="@warehouse.Id">@warehouse.Name </option>
                                                }
                                                    </select>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="stock-transfer-texts" for="middle-name">To:</label>
                                        <div class="col-md-3 col-sm-6 col-xs-12">
                                            <select id="to_warehouse" name=" to_warehouse" class="form-control width-365">
                                                <option value="">Please select any </option>
                                                @foreach (var warehouse in ViewBag.Warehouses)
                                                {
                                                    <option value="@warehouse.Id">@warehouse.Name </option>
                                                }
                                            </select>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="stock-transfer-texts" for="middle-name">QTY:</label>
                                        <div class="col-md-3 col-sm-6 col-xs-12">
                                            <input type="text"  class="width-110"  name="stock_qty" id="quantity">
                                        </div>
                                    </div>


                                </div>

                                <div class="col-md-12">
                                    <div class="col-md-3"></div>

                                    <div class="col-md-3">
                                        <input type="submit" onclick="transferStock();" value="Transfer" class="btn btn-success btn-mini center-block width-123" />
                                    </div>

                                    <div class="col-md-3">
                                        <a href="~/Product/List" class="btn btn-primary btn-mini width-123 center-block a-cancel height-40 mt-0">Cancel</a>
                                    </div>
                                    <div class="col-md-3"></div>



                                </div>
                            </form>


                        </div>


                    </div>
                </div>

                <div class="modal-footer">


                </div>



            </div>
        </div>
    </div>


    @*----------------------------POP UP END STOCK TRANSFER--------------------------------*@


    @*<----------------CATEGORY POP UP------------------>*@
    <div class="modal fade" style="overflow:auto" id="" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg-2" role="document">
            <div class="modal-content modal-border" style="border-radius:10px;background:#e6e6e6;">

                <div class="title col-md-12">
                    <span>Categories</span>
                    <img src="~/img/images/c3.png" class="close mr--13 pull-right" data-dismiss="modal" />
                </div>


                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">

                        <div class="win text-center">
                            @*<h3 class="black-color"><b>Add New Item</b></h3>
                                <img class="pull-right margin-supplier ml-22" src="~/img/images/supplier.png" />*@

                        </div>
                    </div>
                </div>
