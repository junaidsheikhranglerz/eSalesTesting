    @*----------------------------POP UP for ESHOP SETTINGS SETTINGS--------------------------------*@

    <div class="modal fade bs-example-modal-md z-999999999999999999999999" id="eshop_modal" tabindex="-1" role="dialog" aria-hidden="false">
        <div class="modal-dialog modal-lg-2">
            <div class="modal-content modal-style-custom">


                <div class="title col-md-12">
                    <span>Edit-Sync Settings</span>
                    @*<img src="~/img/images/c3.png" class="close mr--13 pull-right" data-dismiss="modal" />*@
                </div>

                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">

                        <div class="win text-center">

                            <div>
                                <h4 class="mt-30 pull-left black-color bold">
                                    Manage Item Syncing
                                    @*<a class="ml-5 mr-5" href="#">
                                            <img class="size-35-32 ml-5 mr-5" src="~/img/images/plus.png" />Add Items
                                        </a>*@
                                </h4>
                            </div>
                            <div class="clearfix"></div>
                            <br />
                            <div class="">

                                <div class="row1 pull-left">
                                    <div class="pull-left"><label>Sync Options  : </label></div>
                                    <div class="pull-left"><label class="mb-0"><input name="syncing" onclick="showBasic()" value="Basic" type="radio" />Basic</label></div>
                                    <div class="pull-left"><label class="mb-0"><input name="syncing" onclick="showStandard()" value="Standard" type="radio" />Standard</label></div>
                                    <div class="pull-left"><label class="mb-0"><input name="syncing" onclick="showAdvance()" value="Advance" type="radio" />Advance</label></div>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                            <div class="clearfix"></div>

                            <div class="">
                                <div id="basic_id" class="none mt-15">
                                    <div class="row1 pull-left">
                                        <div class="pull-left"><label>If warning level reach  : </label></div>
                                        <div class="pull-left"><label class="mb-0"><input name="syncing" value="Basic" type="radio" />Update increased price :</label></div>
                                        <div class="pull-left"><label class="mb-0"><input name="syncing" value="Standard" type="radio" />Put listing Out of Stock :</label></div>
                                    </div>

                                    <form action="" class="form-horizontal form-label-left" method="post">

                                        <div class="col-md-12 margin-stock-modal">

                                            <div class="form-group">
                                                <label class="pull-left width-145" for="middle-name">Warning Level (Qty) :</label>
                                                <div class="col-md-3 col-sm-6 col-xs-12">
                                                    <input type="text" class="width-150" name="stock_qty" id="login">
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="pull-left width-145" for="middle-name">Increased price:</label>
                                                <div class="col-md-3 col-sm-6 col-xs-12">
                                                    <input type="text" class="width-150" name="stock_qty" id="login">
                                                </div>
                                            </div>


                                        </div>


                                    </form>



                                </div>

                                <div id="advance_id" class="none">
                                    <form action="" class="form-horizontal form-label-left" method="post">

                                        <div class="col-md-12 margin-stock-modal">

                                            <div class="form-group">
                                                <label class="pull-left" for="middle-name">Maximum Listed Quantity:</label>
                                                <div class="col-md-3 col-sm-6 col-xs-12">
                                                    <input type="text" class="width-150" name="stock_qty" id="login">
                                                </div>
                                            </div>


                                        </div>


                                    </form>


                                </div>



                                <div class="col-md-12">
                                    <div class="col-md-3"></div>

                                    <div class="col-md-3">
                                        <input type="submit" value="Add" class="btn btn-success btn-mini center-block width-123" />
                                    </div>

                                    <div class="col-md-3">
                                        <a href="~/Product/List" class="btn btn-primary btn-mini width-123 center-block a-cancel height-40 mt-0">Cancel</a>
                                    </div>
                                    <div class="col-md-3"></div>



                                </div>


                            </div>

                        </div>


                    </div>
                </div>

                <div class="modal-footer">


                </div>



            </div>
        </div>
    </div>


    @*----------------------------POP UP END ESHOP SETTINGS--------------------------------*@


    @*----------------------------POP UP ADD PRODUCT--------------------------------*@

    <div class="modal fade" id="CreateModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content modal-style-custom">

                <div class="title col-md-12">
                    <span>Add-Stock</span>
                    <img src="~/img/images/c3.png" class="close mr--13 pull-right" data-dismiss="modal" />
                </div>

                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">

                        <div class="win text-center">
                            <h3 class="black-color mb-40"><b>Add New Item</b></h3>
                            @*<img class="pull-right margin-supplier ml-22" src="~/img/images/supplier.png" />*@

                            <form id="demo-form2" action="@Url.Action("Create","Product")" method="post" data-parsley-validate class="form-horizontal form-label-left" enctype="multipart/form-data">



                                <div class="col-md-12 text-center">

                                    <div class="col-md-9">

                                        <div class="form-group">
                                            <label class="supplier-create-texts" for="middle-name">Custom Label / SKU <span class="required">*</span></label>
                                            <div class="col-md-3 col-sm-6 col-xs-12">
                                                <input class="width-400" type="text" name="sku" required placeholder="SKU is case sensitive. eshop's SKU need to be same.">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="supplier-create-texts" for="middle-name">Item Title <span class="required">*</span></label>
                                            <div class="col-md-3 col-sm-6 col-xs-12">
                                                <input class="width-400" type="text" name="item_title" required>
                                            </div>
                                        </div>



                                        <div class="form-group">
                                            <div>
                                                <div class="">
                                                    <label class="supplier-create-texts" for="middle-name">Normal Price <span class="required">*</span></label>
                                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                                        <input id="NormalPrice" onkeypress="validate(event)" onkeyup="javascript:Add();" class="width-170" type="text" name="normal_price" required>
                                                    </div>
                                                </div>

                                                <div class="">
                                                    <label class="vat-texts">+VAT=</label>
                                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                                        <input id="NormalTotal" onkeypress="validate(event)" type="text" class="width-170" name="normal_vat" required>
                                                    </div>
                                                </div>

                                                <div class="clear"></div>

                                            </div>


                                        </div>




                                        <div class="form-group">
                                            <div>
