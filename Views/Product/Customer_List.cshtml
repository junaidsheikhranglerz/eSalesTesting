    <script>
        $(document).ready(function () {
            $('#customer_list_table').dataTable();
        });
    </script>

    <script>
        function show_customer_summary() {
            $("#customer_summary").removeClass("none");
            $("#customer_summary").addClass("showDiv");
            $("#customer_table_div").addClass("none");
        }
    </script>

    <script>
        function show_customer_type() {
            $("#customer_table_div").removeClass("none");
            $("#customer_table_div").addClass("showDiv");
            $("#customer_summary").addClass("none");
        }
    </script>

    <script>
        $(document).ready(function () {
            $('#customer_invoices_table').dataTable();
        });
    </script>

    <script>
        $(document).ready(function () {
            $('#item_history_table').dataTable();
        });
    </script>

    <script>
        function PrintBtn() {
            window.print();
        }
    </script>

