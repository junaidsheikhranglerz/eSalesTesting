@model List<eSalesPro.Models.Supplier>
<body id="body-index">
    <div id="sticky-footer-wrap">

        @*-----------------Top Navigation Start---------------*@

        @{
            Html.RenderPartial("Top_Navigation_View");
        }

        @*-----------------Top Navigation End---------------*@

        <div class="container-fluid main-container">

            @*-----------------Side Navigation Start---------------*@
            <div class="row">
                @{
                    Html.RenderPartial("Side_Navigation_View");
                }
                @*-----------------Side Navigation End---------------*@



                @*-----------------Main Container Start---------------*@
                <div class="user-page-height">
                    <div class="bggg-img">
                        <div class="ml-10">
                            <h3 class="bold">Suppliers<small></small></h3>
                            <div class="clearfix"></div>
                        </div>

                        <div>

                            <div class="col-md-12">

                                <div class="col-md-3 pull-left">
                                    <a href="~/Home/Epos" class="btn btn-success blue-btn-style btn-mini supplier-epos-btn bold">Back to EPOS Main Panel</a>
                                </div>



                                <div class="col-md-2 col-sm-6 col-xs-6 text-center pull-right existing-supplier-btn-margin">
                                    <a href="#" data-toggle="modal" data-target=".bs-example-modal-md">
                                        <span class="box"><img src="~/img/images/existing_supplier.png" /></span><br />
                                        <span><b>Existing Supplier</b></span>
                                    </a>
                                </div>

                                <div class="col-md-2 col-sm-6 col-xs-6 text-center pull-right new-supplier-btn-margin">
                                    <a href="#" data-toggle="modal" data-target="#CreateModal">
                                        @*<a href="~/Supplier/SuppCreate">*@
                                        <span class="box"><img src="~/img/images/new_supplier.png" /></span><br />
                                        <span><b>New Supplier</b></span>

                                    </a>
                                </div>

                            </div>
                        </div>


                        <div class="clearfix"></div>


                        @*<section id="no-more-tables">*@
                        <section id="no-more-tables" class="mt-15 table-inline-margin">

                            <div class="table-border">


                                <table border="0" class="table table-responsive col-md-12">
                                    <thead>
                                        <tr class="table-heading-fonts table-main-headings">
                                            <th>ID</th>
                                            <th>Salesman</th>
                                            <th>Phone</th>
                                            <th>Email</th>
                                            <th>Address</th>
                                            <th>Postcode</th>

                                            <th>Status</th>

                                            @*<th>Pay Rate</th>
                        <th>Role</th>
                        <th>Status</th>*@

                                            <th class="text-center">Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody class="black-color bold">

                                        @foreach (var v in Model)
                                    {
                                    @*<tr data-toggle="modal" data-target="#EditModal">*@

                                      <tr>
                                        <td data-title="User ID">@v.Id</td>
                                        <td data-title="User ID">@v.Name</td>
                                        <td data-title="First Name">@v.Phone</td>
                                        <td data-title="Last Name">@v.Email</td>
                                        <td data-title="Email" class="user-emails">@v.Address</td>
                                        <td data-title="Date Registered">@v.Post_Code</td>

                                        @if (@v.Is_Active ==1)
                                            {
                                            <td class="gg">Active</td>
                                            }
                                            else
                                            {
                                            <td class="red">Deactive</td>
                                            }

                                        <td data-title="Actions" style="min-width:191px" class="text-center width-193 buttons-block">
                                            <a class="btn btn-sm btn-info bold" href="" data-toggle="modal" data-target="#EditModal" onclick="GetEditedObject(@v.Id)">
                                                Edit
                                            </a>
                                            <a class="btn btn-sm btn-success bold" href="~/Supplier/Active/@v.Id">
                                                Enable
                                            </a>
                                            <a class="btn btn-sm btn-danger bold" href="~/Supplier/Disable/@v.Id">
                                                Disable
                                            </a>
                                        </td>
                                    </tr>
                                    }
                                    </tbody>

                                </table>

                                <div class="clearfix"></div>


                            </div>

                    </section>







                </div>
                </div>
                @*-----------------Main Container Ends---------------*@
            </div>


            <div id="sticky-footer-push"></div>
        </div>
    </div>

    <footer class="black">

        @{
            Html.RenderPartial("Footer_View");
        }
    </footer>



