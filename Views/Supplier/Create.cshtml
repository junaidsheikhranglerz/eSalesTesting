@model List<eSalesPro.Models.Supplier>
<body id="body-index">
    <div id="sticky-footer-wrap">

        @*-----------------Top Navigation Start---------------*@

        @{
            Html.RenderPartial("Top_Navigation_View");
        }

        @*-----------------Top Navigation End---------------*@

        <div class="container-fluid main-container">

            @*-----------------Side Navigation Start---------------*@
            <div class="row">
                @{
                    Html.RenderPartial("Side_Navigation_View");
                }
                @*-----------------Side Navigation End---------------*@



                @*-----------------Main Container Start---------------*@
                <div class="right-side-custom">
                    <div class="bggg-img">
                        <div class="" style="margin-left:12px">
                            <h3>Suppliers<small></small></h3>
                            <div class="clearfix"></div>
                        </div>

                        <div>

                            <div class="col-md-12">

                                <div class="col-md-3 pull-left">
                                    <a href="~/Home/Epos" class="btn btn-success btn-mini" style="margin:15px 0 0 -20px">Back to EPOS Main Panel</a>
                                </div>



                                <div class="col-md-2 col-sm-6 col-xs-6 text-center pull-right" style="margin:-20px -25px 0 0">
                                    <a href="#" data-toggle="modal" data-target=".bs-example-modal-md">
                                        <span class="box"><img src="~/img/images/existing_supplier.png" /></span><br />
                                        <span><b>Existing Supplier</b></span>
                                    </a>
                                </div>

                                <div class="col-md-2 col-sm-6 col-xs-6 text-center pull-right" style="margin:-20px -40px 0 0">
                                    @*<a href="#" data-toggle="modal" data-target="#CreateModal">*@
                                    <a href="~/Supplier/SuppCreate">
                                        <span class="box"><img src="~/img/images/new_supplier.png" /></span><br />
                                        <span><b>New Supplier</b></span>

                                    </a>
                                </div>

                            </div>
                        </div>


                        <section id="no-more-tables" style="margin:50px 12px 0 12px">
                            <table class="table table-responsive col-md-12" style="margin-top:30px">
                                <thead>
                                    <tr style="color:#0094ff; font-size:16px">
                                        <th>ID</th>
                                        <th>Full Name</th>
                                        <th>Username</th>
                                        <th>Password</th>
                                        <th>Phone</th>
                                        <th>Pay Rate</th>
                                        <th>Role</th>
                                        <th>Status</th>

                                        @*<th>Pay Rate</th>
                                        <th>Role</th>
                                        <th>Status</th>*@

                                        <th class="text-center">Actions</th>
                                    </tr>
                                </thead>
                                <tbody style="font-size:15px; color:#000">

                                    @foreach (var v in Model)
                                    {
                                    
                                        <tr>
                                    @*<tr data-toggle="modal" data-target="#editModal">*@

                                        <td data-title="User ID">@v.Id</td>
                                        <td data-title="User ID">@v.Name</td>
                                        <td data-title="First Name">@v.Company_Id</td>
                                        <td data-title="Last Name">@v.Password</td>
                                        <td data-title="Email" class="user-emails">@v.Phone</td>
                                        <td data-title="Date Registered">@v.Phone_Ext</td>
                                        <td data-title="Date Registered">@v.Post_Code</td>
                                        @if (@v.Is_Active == 1)
                                            {
                                            <td>Active</td>
                                            }
                                            else
                                            {
                                            <td>Deactive</td>
                                            }

                                        <td data-title="Actions" class="text-center buttons-block">
                                            <a class="btn btn-sm btn-success" href="~/Supplier/Edit">
                                                Edit
                                            </a>
                                            <a class="btn btn-sm btn-info" href="">
                                                Enable
                                            </a>
                                            <a class="btn btn-sm btn-danger" href="">
                                                Disable
                                            </a>
                                        </td>
                                    </tr>
                                    }
                                </tbody>

                            </table>

                        </section>







                    </div>
                </div>
                @*-----------------Main Container Ends---------------*@
            </div>
            <div id="sticky-footer-push"></div>
        </div>
    </div>

    <footer class="black">

        @{
            Html.RenderPartial("Footer_View");
        }
    </footer>



