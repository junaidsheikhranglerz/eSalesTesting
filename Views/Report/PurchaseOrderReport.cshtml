                                            <th>Ordered Total+Vat</th>
                                            <th>Received Total+Vat</th>
                                            <th>Date</th>
                                            <th>Pay Priority</th>
                                            <th>Payment Method</th>
                                            <th>Pay Status</th>
                                            <th>PO Status</th>


                                        </tr>
                                    </thead>
                                    <tbody class="black-color bold">

                                        @*@foreach (var v in Model)
